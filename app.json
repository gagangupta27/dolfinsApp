{
  "expo": {
    "name": "Dolfins",
    "slug": "dolfins",
    "scheme": "dolfins",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "accessesContactNotes": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.dolfins.ai",
      "infoPlist": {
        "NSContactsUsageDescription": "Allow $(PRODUCT_NAME) to access your contacts.",
        "LSApplicationQueriesSchemes": [
          "mailto"
        ],
        "ITSAppUsesNonExemptEncryption": "NO"
      },
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dolfins.ai",
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "output": "server",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "react-native-auth0",
        {
          "domain": "{AUTH0_DOMAIN}"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "dolfins"
        }
      ],
      [
        "react-native-cloud-store",
        {
          "iCloud": {
            "kv": {
              "enabled": true
            },
            "documents": [
              {
                "containerId": "iCloud.com.dolfins.ai",
                "enabled": true,
                "visible": true,
                "visibleName": "myDisplayName"
              },
              {
                "containerId": "iCloud.com.dolfins.ai",
                "enabled": true,
                "visible": false
              }
            ]
          }
        }
      ],
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "20c8b718-ece1-4162-bdbb-1f6321e43caf"
      }
    },
    "owner": "siddharthbulia"
  }
}
